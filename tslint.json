{
  "defaultSeverity": "none",
	"extends": [
		"tslint:recommended",
		"tslint-react"
	],
	"rules": {
		"ordered-imports": false,
		"interface-name": false,
		"no-console": false,
		"jsx-no-multiline-js": false,
		"object-literal-sort-keys": false,
		"member-access": false,
		"no-var-requires": false,
		"jsx-no-lambda": false
	}
}